FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "3200";
    object      gradK;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -2 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    low
    {
        type            extrapolatedCalculated;
        value           uniform (0 0 0);
    }
    left
    {
        type            extrapolatedCalculated;
        value           uniform (0 0 0);
    }
    rifht
    {
        type            extrapolatedCalculated;
        value           uniform (0 0 0);
    }
    up
    {
        type            extrapolatedCalculated;
        value           uniform (0 0 0);
    }
    frontAndBackPlanes
    {
        type            empty;
    }
}


